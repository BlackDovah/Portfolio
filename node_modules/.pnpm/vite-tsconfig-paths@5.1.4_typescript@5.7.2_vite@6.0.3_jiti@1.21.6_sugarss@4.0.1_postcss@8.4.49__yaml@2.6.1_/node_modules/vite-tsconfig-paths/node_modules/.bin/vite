#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/blackdovah/Documents/GitHub/Mantine-Vite-template/node_modules/.pnpm/vite@6.0.3_jiti@1.21.6_sugarss@4.0.1_postcss@8.4.49__yaml@2.6.1/node_modules/vite/bin/node_modules:/home/blackdovah/Documents/GitHub/Mantine-Vite-template/node_modules/.pnpm/vite@6.0.3_jiti@1.21.6_sugarss@4.0.1_postcss@8.4.49__yaml@2.6.1/node_modules/vite/node_modules:/home/blackdovah/Documents/GitHub/Mantine-Vite-template/node_modules/.pnpm/vite@6.0.3_jiti@1.21.6_sugarss@4.0.1_postcss@8.4.49__yaml@2.6.1/node_modules:/home/blackdovah/Documents/GitHub/Mantine-Vite-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/blackdovah/Documents/GitHub/Mantine-Vite-template/node_modules/.pnpm/vite@6.0.3_jiti@1.21.6_sugarss@4.0.1_postcss@8.4.49__yaml@2.6.1/node_modules/vite/bin/node_modules:/home/blackdovah/Documents/GitHub/Mantine-Vite-template/node_modules/.pnpm/vite@6.0.3_jiti@1.21.6_sugarss@4.0.1_postcss@8.4.49__yaml@2.6.1/node_modules/vite/node_modules:/home/blackdovah/Documents/GitHub/Mantine-Vite-template/node_modules/.pnpm/vite@6.0.3_jiti@1.21.6_sugarss@4.0.1_postcss@8.4.49__yaml@2.6.1/node_modules:/home/blackdovah/Documents/GitHub/Mantine-Vite-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite@6.0.3_jiti@1.21.6_sugarss@4.0.1_postcss@8.4.49__yaml@2.6.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../vite@6.0.3_jiti@1.21.6_sugarss@4.0.1_postcss@8.4.49__yaml@2.6.1/node_modules/vite/bin/vite.js" "$@"
fi
